{
    "controllers": {
        "@survos/grid-bundle": {
            "grid": {
                "enabled": true,
                "fetch": "lazy"
            },
            "api_grid": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@symfony/ux-autocomplete": {
            "autocomplete": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "tom-select/dist/css/tom-select.default.css": true
                }
            }
        }
    },
    "entrypoints": []
}
